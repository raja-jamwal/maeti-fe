{
  "expo": {
    "name": "matrimony",
    "slug": "matrimony",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "primaryColor": "#481f6f",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#481f6f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "matrimony.datagrids.in",
      "googleServicesFile": "./google-services.json"
    },
    "androidStatusBar": {
      "backgroundColor": "#481f6f",
      "barStyle": "light-content"
    }
  }
}